#, no-wrap
msgid "\tinp\t0.78634\t0.693 ~\n"
msgstr ""

#. type: Plain text
#: original/TXT/usr_25.txt:565
msgid "The \"gR\" command uses Virtual Replace mode.  This preserves the layout:"
msgstr ""

#. type: Plain text
#: original/TXT/usr_25.txt:571
#, no-wrap
msgid ""
"\t\t|\n"
"\tgR0.786 |\n"
"\t\tV\n"
msgstr ""
"\t\t|\n"
"\tgR0.786 |\n"
