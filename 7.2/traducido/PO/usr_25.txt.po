#. type: Plain text
#: original/TXT/usr_25.txt:243
#, fuzzy, no-wrap
msgid ""
"\tthe first line ~\n"
"\t    the second line ~\n"
msgstr ""
"\tEsto es una línea larga ~\n"
"\tcorta ~\n"
"\tCualquier es una línea larga ~\n"

#. type: Plain text
#: original/TXT/usr_25.txt:245
msgid "\"4>>\" will increase the indent of four lines."
